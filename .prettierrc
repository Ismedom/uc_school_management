{
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "plugins": ["prettier-plugin-java"],
    "overrides": [
        {
            "files": "*.java",
            "options": {
                "printWidth": 120,
                "tabWidth": 4
            }
        }
    ]
}
